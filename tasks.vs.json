{
  "version": "0.2.1",
  "tasks": [
    {
      "appliesTo": "makefile",
      "type": "launch",
      "taskName": "makefile-build",
      "contextType": "build",
      "workingDirectory": "${workspaceRoot}",
      "command": "${env.MSYS2_ROOT}/usr/bin/bash",
      "args": [
        "-lc \"./make_vs.sh all\""
      ],
      "output": "bin/dpmitest.exe"
    },
    {
      "appliesTo": "makefile",
      "type": "launch",
      "taskName": "makefile-rebuild",
      "contextType": "rebuild",
      "workingDirectory": "${workspaceRoot}",
      "command": "${env.MSYS2_ROOT}/usr/bin/bash",
      "args": [
        "-lc \"./make_vs.sh -B all\""
      ],
      "output": "bin/dpmitest.exe"
    },
    {
      "appliesTo": "makefile",
      "type": "launch",
      "taskName": "makefile-clean",
      "contextType": "clean",
      "workingDirectory": "${workspaceRoot}",
      "command": "${env.MSYS2_ROOT}/usr/bin/bash",
      "args": [
        "-lc \"./make_vs.sh clean\""
      ]
    }
  ]
}
