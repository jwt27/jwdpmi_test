{
  "version": "0.2.1",
  "tasks": [
    {
      "appliesTo": "makefile",
      "type": "launch",
      "taskLabel": "makefile-build",
      "contextType": "build",
      "workingDirectory": "${workspaceRoot}",
      "command": "${workspaceRoot}/make_vs.bat",
      "args": [
        "${workspaceRoot}",
        "all"
      ],
      "output": "bin/dpmitest.exe"
    },
    {
      "appliesTo": "makefile",
      "type": "launch",
      "taskLabel": "makefile-rebuild",
      "contextType": "rebuild",
      "workingDirectory": "${workspaceRoot}",
      "command": "${workspaceRoot}/make_vs.bat",
      "args": [
        "${workspaceRoot}",
        "-B",
        "all"
      ],
      "output": "bin/dpmitest.exe"
    },
    {
      "appliesTo": "makefile",
      "type": "launch",
      "taskLabel": "makefile-clean",
      "contextType": "clean",
      "workingDirectory": "${workspaceRoot}",
      "command": "${workspaceRoot}/make_vs.bat",
      "args": [
        "${workspaceRoot}",
        "clean"
      ]
    }
  ]
}
