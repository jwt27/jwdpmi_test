{
  "version": "0.2.1",
  "tasks": [
    {
      "appliesTo": "makefile",
      "type": "launch",
      "taskName": "build",
      "contextType": "build",
      "workingDirectory": "${workspaceRoot}",
      "command": "${env.MSYS2_ROOT}/usr/bin/bash",
      "args": [
        "-lc \"./make_vs.sh -j24 all\""
      ]
    },
    {
      "appliesTo": "makefile",
      "type": "launch",
      "taskName": "rebuild",
      "contextType": "rebuild",
      "workingDirectory": "${workspaceRoot}",
      "command": "${env.MSYS2_ROOT}/usr/bin/bash",
      "args": [
        "-lc \"./make_vs.sh -B -j24 all\""
      ]
    },
    {
      "appliesTo": "makefile",
      "type": "launch",
      "taskName": "clean",
      "contextType": "clean",
      "workingDirectory": "${workspaceRoot}",
      "command": "${env.MSYS2_ROOT}/usr/bin/bash",
      "args": [
        "-lc \"./make_vs.sh clean\""
      ]
    }
  ]
}
